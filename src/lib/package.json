{
    "$schema": "../../node_modules/ng-packagr/package.schema.json",
    "name": "@zokelion/ngx-countdown",
    "version": "1.0.0",
    "description": "ngx-countdown is a light and easy to use Angular library that will give you access to a countdown display component",
    "repository": {
        "type": "git",
        "url": "https://github.com/Zokelion/ngx-countdown"
    },
    "contributors": [
        "Pierrick Couget <pierrick.couget@gmail.com>"
    ],
    "license": "MIT",
    "keywords": [
        "countdown",
        "timer",
        "angular"
    ],
    "peerDependencies": {
        "@angular/animations": "^6.0.0 || ^7.0.0",
        "@angular/router": "^6.0.0 || ^7.0.0",
        "@angular/common": "^6.0.0 || ^7.0.0",
        "@angular/core": "^6.0.0 || ^7.0.0",
        "@angular/platform-browser": "^6.0.0 || ^7.0.0",
        "@fortawesome/angular-fontawesome": "^0.2.0",
        "@fortawesome/fontawesome-svg-core": "^1.2.4",
        "@fortawesome/free-solid-svg-icons": "^5.3.1"
    },
    "ngPackage": {
        "lib": {
            "entryFile": "public_api.ts",
            "flatModuleFile": "ngx-countdown"
        },
        "dest": "../../dist"
    }
}
